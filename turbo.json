{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "ACCOUNT",
    "STARKNET_CORE_L1_ADDRESS",
    "PRIVATE_KEY",
    "GOERLI_PRIVATE_KEY",
    "ETHERSCAN_API_KEY",
    "ALCHEMY_API_KEY"
  ],
  "pipeline": {
    "build:l1": {},
    "deploy:l1": {},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"]
    }
  }
}
